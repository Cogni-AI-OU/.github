{
  "problemMatcher": [
    {
      "owner": "pre-commit-generic",
      "pattern": [
        {
          "regexp": "^(.+?):(\\d+):(\\d+): (.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ]
    },
    {
      "owner": "pre-commit-no-column",
      "pattern": [
        {
          "regexp": "^(.+?):(\\d+) (.+)$",
          "file": 1,
          "line": 2,
          "message": 3
        }
      ]
    }
  ]
}
