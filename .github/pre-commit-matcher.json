{
  "problemMatcher": [
    {
      "owner": "yamllint",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^(.+?):(\\d+):(\\d+): \\[(warning|error)\\] (.+) \\((.+)\\)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5,
          "code": 6
        }
      ]
    },
    {
      "owner": "flake8",
      "pattern": [
        {
          "regexp": "^(.+?):(\\d+):(\\d+): ([A-Z]\\d+) (.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "markdownlint",
      "pattern": [
        {
          "regexp": "^(.+?):(\\d+)(?::(\\d+))? (MD\\d+/[\\w-]+) (.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "codespell",
      "pattern": [
        {
          "regexp": "^(.+?):(\\d+): (.+)$",
          "file": 1,
          "line": 2,
          "message": 3
        }
      ]
    },
    {
      "owner": "ansible-lint",
      "pattern": [
        {
          "regexp": "^(.+?):(\\d+)(?::(\\d+))?: \\[(.+)\\] (.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "black",
      "pattern": [
        {
          "regexp": "^(would reformat|error:) (.+)$",
          "file": 2,
          "message": 1
        }
      ]
    },
    {
      "owner": "pre-commit-generic",
      "pattern": [
        {
          "regexp": "^(.+?):(\\d+):(\\d+): (.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ]
    }
  ]
}
